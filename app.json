{
  "expo": {
    "name": "E-ticket",
    "slug": "E-ticket",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/E_ticket.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/E_ticket.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/E_ticket.png",
        "backgroundColor": "#000000"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-font", "expo-secure-store"]
  }
}
